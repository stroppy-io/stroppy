{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "lib": ["ES2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "types": ["node", "vitest/globals"],
    "allowSyntheticDefaultImports": true,
    "rootDir": "..",
    "baseUrl": ".."
  },
  "include": ["../**/*.ts", "**/*.ts"],
  "exclude": ["node_modules"]
}

